{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "types.d.ts",
    "app/api/cars.js",
    "app/components/Herosection.jsx",
    "app/components/Herosection.jsx",
    "app/car-detail/[slug]/page.jsx",
    "app/components/Slider.jsx",
    "app/page.jsx",
    "app/components/LayoutRenderer.jsx",
    "app/signup/page.jsx",
    "app/login/page.jsx",
    "app/api/cars/route.js"
, "app/api/blog/slug/[slug]/route.ts", "app/components/Cookiebox.tsx", "app/components/SidebarFilters.tsx", "app/api/users/favorites/route.ts"  ],
  "exclude": ["node_modules"]
}
